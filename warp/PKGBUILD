pkgname='warp'
arch=('x86_64')
pkgdesc='cloudflare warp'
pkgrel=1
pkgver=2025.6.1335.0
url='https://cloudflare.com'
_rpmname="CloudflareWarp-${pkgver}_x86_64.rpm"
source=("https://downloads.cloudflareclient.com/v1/download/fedora35-intel/version/${pkgver}")
sha256sums=('5369aaf2b0bcff975e6f7c4314733e58e7c98bdd43500bfab1325e568a38bcff')
_projectname="$pkgname"

package() {
    cp -r "${srcdir}/opt" "${pkgdir}/"
    cp -r "${srcdir}/bin" "${pkgdir}/"
}
